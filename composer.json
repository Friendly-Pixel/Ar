{
    "name": "frontwise/ar",
    "description": "Consistent array functions",
    "homepage": "https://github.com/frontwise/ar/",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Frontwise\\Ar\\": "src",
            "Frontwise\\Ar\\Test\\": "tests"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "bash ./githooks/install.sh"
        ],
        "post-update-cmd": [
            "bash ./githooks/install.sh"
        ]
    }
}
